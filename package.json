  },
  "bugs": {
    "url": "https://github.com/paulshryock/New-Project-Starter-Kit/issues"
  },
  "scripts": {
    "start": "npm run build",
    "develop": "NODE_ENV='development' gulp develop",
    "serve": "NODE_ENV='development' gulp serve",
    "build": "NODE_ENV='production' gulp",
    "production": "NODE_ENV='production' gulp production",
    "update": "ncu -u && npm update",
    "now-build": "npm start",
    "audit:performance": "node ./_scripts/lighthouse.js",
    "create:content": "node ./_scripts/addContentType.js",
    "update:gitignore": "node ./_scripts/ignorePlatforms.js"
  },
